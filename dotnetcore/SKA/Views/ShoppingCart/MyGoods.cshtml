@model IEnumerable<goods>
@{<table class="table table-striped">
      <tr>
          <td>@Html.DisplayNameFor(Model => Model.Name)</td>
          <td>@Html.DisplayNameFor(Model => Model.Price)</td>
          <td>@Html.DisplayNameFor(Model => Model.num)</td>
          <td></td>
      </tr>
    @foreach (var item in Model)
    {
  <tr>
      <td>@Html.DisplayFor(modelItem => item.Name)</td>
      <td>@Html.DisplayFor(modelItem => item.Price)</td>
      <td>@Html.DisplayFor(modelItem => item.num)</td"col">
      <td>@Html.ActionLink("delete", "deleteMyGoods", routeValues: new { item.Name })</td>
  </tr>
    }
    </table>
}
